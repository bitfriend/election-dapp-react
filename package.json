{
  "name": "election-dapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.13.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "3.4.4",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.2.6",
    "bip39": "^3.0.3"
  },
  "scripts": {
    "start": "set REACT_APP_NETWORK_NAME=development&&react-scripts start",
    "start:ropsten": "set REACT_APP_NETWORK_NAME=ropsten&&react-scripts start",
    "start:kovan": "set REACT_APP_NETWORK_NAME=kovan&&react-scripts start",
    "start:rinkeby": "set REACT_APP_NETWORK_NAME=rinkeby&&react-scripts start",
    "start:mainnet": "set REACT_APP_NETWORK_NAME=mainnet&&react-scripts start",
    "build": "set REACT_APP_NETWORK_NAME=development&&react-scripts build",
    "build:ropsten": "set REACT_APP_NETWORK_NAME=ropsten&&react-scripts build",
    "build:kovan": "set REACT_APP_NETWORK_NAME=kovan&&react-scripts build",
    "build:rinkeby": "set REACT_APP_NETWORK_NAME=rinkeby&&react-scripts build",
    "build:mainnet": "set REACT_APP_NETWORK_NAME=mainnet&&react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "mnemonic": "node -e \"console.log(require('bip39').generateMnemonic())\"",
    "compile": "truffle compile",
    "deploy": "truffle migrate --reset"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
